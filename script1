pipeline {
    agent none
    stages {
        stage ('ALL') 
        parallel {
            stage ('build') { 
                agent { label 'master' }
                steps {
                 sh''' 
                echo "build stage"
             '''
        }
   }
       stage ('deploy') {
        agent { label 'master'}
        steps {
           sh'''
           echo "deploy stage"
           '''
     }
    }
   }
 }
}     
